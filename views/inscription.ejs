<main>
<div class="insContainer">
    <form action="/" method="post"></form>
        <h1>Inscription</h1>
        <div class="identity">
            <div class="nom">
                <h2>Nom</h2>
                <div class="donneesIns">
                    <input type="text" name="nom" placeholder="nom de famille"/>
                </div>
            </div>
            <div class="prenom">
                <h2>Prénom</h2>
                <div class="donneesIns">
                    <input type="text" name="prenom" placeholder="Votre prénom"/>
                </div>
            </div>
        </div>
        <div class="prenom">
            <h2>Date de naissance</h2>
            <div class="donneesIns">
                <input type="text" name="ddn" placeholder="jj/mm/aa"/>
            </div>
        </div>
        <div class="adresse">
            <h2>Adresse mail</h2>
            <div class="donneesIns">
                <input type="text" name="adresseMail" placeholder="Votre adresse mail"/>
            </div>
        </div>
        <div class="adressePostale">
            <div class="nom">
                <h2>Mot de passe</h2>
                <div class="donneesIns">
                    <input type="text" name="MDP" placeholder="Votre mot de passe"/>
                </div>
            </div>
            <div class="nom">
                <h2>Confirmer votre mot de passe</h2>
                <div class="donneesIns">
                    <input type="text" name="VMDP" placeholder="Confirmer le mot de passe"/>
                </div>
            </div>
            <button class="input" type="submit">Inscription</button>
        </div>
    </form>
</div>
</main>
